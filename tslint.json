{
  "rules": {
    "no-console": false,
    "indent": [true, "spaces", 2],
    "semicolon": [true, "never"],
    "interface-name": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-debugger": false
  },
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.tsx",
      "node_modules/**/*.ts"
    ]
  }
}
